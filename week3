LARGER POWER
def is_power_greater(base, exponent):
    """
    Determines if base^exponent exceeds 5000.
    
    Args:
        base (int/float): The base number
        exponent (int/float): The exponent
    
    Returns:
        bool: True if result > 5000, False otherwise
    """
    result = base ** exponent
    if result > 5000:
        return True
    else:
        return False

# Example usage:
print(is_power_greater(10, 4))   # Output: True (10⁴ = 10,000)
print(is_power_greater(5, 5))    # Output: False (5⁵ = 3,125)
print(is_power_greater(8, 4))    # Output: False (8⁴ = 4,096)
print(is_power_greater(9, 4))    # Output: True (9⁴ = 6,561)

 CODING SECTION
def large_power(base, exponent):
    return base ** exponent > 5000


DIVISIBLE BY 10
def divisible_by_ten(num):
    remainder = num % 10
    if remainder == 0:
        return True
    else:
        return False

CODING SECTION
def divisible_by_ten(num):
    return num % 10 == 0

